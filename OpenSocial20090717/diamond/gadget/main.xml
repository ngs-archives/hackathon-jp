<?xml version="1.0" encoding="utf-8"?>

<Module>

<ModulePrefs title="mobako">
    <Require feature="opensocial-0.8" />
    <Require feature="views" />
    <Require feature="dynamic-height" />
    <Optional feature="content-rewrite">
        <Param name="expires">0</Param>
        <Param name="include-urls"></Param>
        <Param name="exclude-urls">.*</Param>
    </Optional>
</ModulePrefs>

<Content type="html" view="canvas"><![CDATA[

<link rel="stylesheet" href="http://codess.heteml.jp/diamond/gadget/gadget.css" type="text/css" />
<link rel="stylesheet" href="http://codess.heteml.jp/diamond/gadget/main.css?3" type="text/css" />

<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.3.1/jquery.min.js"></script>
<script type="text/javascript" src="http://codess.heteml.jp/diamond/gadget/gadget.js"></script>
<script type="text/javascript" src="http://codess.heteml.jp/diamond/gadget/main.js?6"></script>
<script type="text/javascript" src="http://codess.heteml.jp/diamond/gadget/ui.js?6"></script>
<script type="text/javascript">
cods.properties.debug = true;

gadgets.util.registerOnLoadHandler(diamond.init);
</script>


<div id="roads">
    <div id="road1" class="road">
        <div>1</div>
        <div class="result"></div>
    </div>
    <div id="road2" class="road">
        <div>2</div>
        <div class="result"></div>
    </div>
    <div id="road3" class="road">
        <div>3</div>
        <div class="result"></div>
    </div>
    <div id="road4" class="road">
        <div>4</div>
        <div class="result"></div>
    </div>
    <div id="road5" class="road">
        <div>5</div>
        <div class="result"></div>
    </div>
    <div style="clear: both;"></div>
</div>


<div id="players"></div>


<div id="cards"></div>


<div id="buttons">
    <div><a href="javascript:diamond.go()">進む</a></div>
    <div><a href="javascript:diamond.exit()">帰る</a></div>
</div>


<div id="count"></div>


]]></Content>

</Module>