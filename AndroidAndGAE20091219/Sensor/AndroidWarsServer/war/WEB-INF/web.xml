<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>ReceiveEventServlet</servlet-name>
		<servlet-class>com.example.androidwars.server.servlets.ReceiveEventServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ReceiveEventServlet</servlet-name>
		<url-pattern>/recieveEvent</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ResponseEventsServlet</servlet-name>
		<servlet-class>com.example.androidwars.server.servlets.ResponseEventsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ResponseEventsServlet</servlet-name>
		<url-pattern>/responseEvents</url-pattern>
	</servlet-mapping>


	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
