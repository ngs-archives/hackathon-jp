<?xml version="1.0" encoding="UTF-8"?>
<Module>
    <ModulePrefs
            title="kamiya_gaget"
            author_email="yusuke.kamiya@drecom.co.jp"
            description="test gaget by kamiya">
        <Require feature="opensocial-0.8" />
        <Require feature="dynamic-height" />
    </ModulePrefs>



    <Content type="html" view="canvas"><![CDATA[
<script type='text/javascript' src='/gaget_test/canvas.js'></script>
<script type='text/javascript'>init();</script>

<div>
  <div>
    <img src="/gaget_test/logo.gif" border="0" />
  </div>
  <ul id='contents'></ul>
  <div id='friend_view'>
    <input type='text' id='todo_content' style="width:600px; font-size:30px; border:2px solid #993333; color:#ff9966;" /><br><br>
    <input type='image' onclick='addTodo();' src="/gaget_test/image/happy_big.jpg" />
    <div id='result_appdata'></div>
  </div>
</div>

<script type='text/javascript'>showTodoList();</script>
]]></Content>





    <Content type="html" view="profile"><![CDATA[
<script type='text/javascript' src='/gaget_test/canvas.js'></script>
<script type='text/javascript'>init();</script>

<div>
  <div>
    <img src="/gaget_test/logo.gif" border="0" />
  </div>
  <ul id='contents'></ul>
  <div id='friend_view'>
    <input type='text' id='todo_content' style="width:300px; font-size:30px; border:2px solid #993333; color:#ff9966;" /><br><br>
    <input type='image' onclick='addTodo();' src="/gaget_test/image/pushme.gif" />
    <div id='result_appdata'></div>
  </div>
</div>

<script type='text/javascript'>showTodoList();</script>
]]></Content>






    <Content type="html" view="preview"><![CDATA[

<img src="/gaget_test/logo.gif" border="0" />

]]></Content>




    <Content type="html" view="home"><![CDATA[

<img src="/gaget_test/logo.gif" border="0" />

]]></Content>


</Module>
