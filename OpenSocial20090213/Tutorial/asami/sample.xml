<?xml version="1.0" encoding="UTF-8"?>
<Module>
  <ModulePrefs title="sample">
    <Require feature="opensocial-0.8"/>
    <Require feature="views" />
  </ModulePrefs>
  <Content type="html">
    <![CDATA[
 	<script>
		
		var defaultItemList = ['ごはん', 'しお', 'ミソ', '小麦粉'];
		
		gadgets.util.registerOnLoadHandler(init);

		function init() {
		  onLoadItems();
		}
		
		function onLoadItems() {

		  html = new Array();
		  html.push('<ul>');
		  for(i = 0 ; i < defaultItemList.size ; i++) {
		  	html.push('<li>' + defaultItemList[i] + "</li>");
		  };
		  html.push('</ul>');
		  document.getElementById('items').innerHTML = html.join('');
		}
		
		
	</script>
	
	<div id='main'>
	        items:
			<div id='items'></div>
	</div>
    ]]>
  </Content>
</Module>
