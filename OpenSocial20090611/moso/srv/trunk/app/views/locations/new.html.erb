<h1>New location</h1>

<% form_for(@location) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :setX %><br />
    <%= f.text_field :setX %>
  </p>
  <p>
    <%= f.label :setY %><br />
    <%= f.text_field :setY %>
  </p>
  <p>
    <%= f.label :setPhoto %><br />
    <%= f.text_field :setPhoto %>
  </p>
  <p>
    <%= f.label :comment %><br />
    <%= f.text_field :comment %>
  </p>
  <p>
    <%= f.label :kinds %><br />
    <%= f.text_field :kinds %>
  </p>
  <p>
    <%= f.label :userId %><br />
    <%= f.text_field :userId %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', locations_path %>
